Gval TEMPO2 Let $BEADvol
Gval TEMPO2 Add 5
If $TEMPO2 LT 15
  Gval TEMPO2 Let 15
EndIf
Do !$TEMPO2(NumberOfInTipMixes)
  WaitMsec #MIX_DELAY2
  LiqOut   $TEMPO1   #P_SPEED_M
  WaitMsec #MIX_DELAY1
  LiqIn    $TEMPO1   #P_SPEED_H
LOOP
LiqOut #V_MAG_PASS #P_SPEED_L
ActionZ #Z_SPEED_H
LiqOutWell% 80 #P_SPEED_M
WaitSec 1
LiqOutAll #P_SPEED_H
LiqOut @TipVolume #P_SPEED_H

